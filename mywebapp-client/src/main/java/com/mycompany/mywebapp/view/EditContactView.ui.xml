<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:dt='urn:import:com.mycompany.mywebapp.editor' >
<ui:style src="../common/common.css"/>
<g:HTMLPanel>
    <g:DockLayoutPanel unit="PX" height="398px">
        <g:center size="100">
            <dt:EditContactEditor ui:field="contactEditor"></dt:EditContactEditor>
        </g:center>
        <g:south size="67.0">
            <g:FlowPanel>
                <g:Button styleName="{style.important}" ui:field="saveButton" text="Save"/>
                <g:Button styleName="{style.important}" ui:field="cancelButton" text="Cancel"/>
            </g:FlowPanel>
        </g:south>
    </g:DockLayoutPanel>

</g:HTMLPanel>
</ui:UiBinder>
